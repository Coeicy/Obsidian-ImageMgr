{
	"name": "imagemgr",
	"version": "0.1.0",
	"description": "强大的 Obsidian 图片管理插件，支持智能扫描、批量重命名、MD5 去重、引用追踪、回收站等功能。",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"dev:watch": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"obsidian-plugin",
		"image-management",
		"batch-rename",
		"md5-deduplication"
	],
	"author": "Coeicy",
	"repository": {
		"type": "git",
		"url": "https://github.com/Coeicy/Obsidian-ImageMgr.git"
	},
	"bugs": {
		"url": "https://github.com/Coeicy/Obsidian-ImageMgr/issues"
	},
	"homepage": "https://github.com/Coeicy/Obsidian-ImageMgr#readme",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@types/spark-md5": "^3.0.5",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"spark-md5": "^3.0.2"
	}
}
